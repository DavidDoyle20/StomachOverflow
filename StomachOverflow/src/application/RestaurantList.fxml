              </columns>
            </TableView>
            <Button layoutX="27.0" layoutY="150.0" mnemonicParsing="false" onAction="#seeRestaurant" prefHeight="16.0" prefWidth="200.0" text="See Selected Restaurant" />
         </children>
      </Pane>
      <ImageView fitHeight="57.0" fitWidth="200.0" layoutX="14.0" layoutY="10.0">
         <image>
            <Image url="@stomachoverflow.png" />
         </image>
      </ImageView>
   </children>
