<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RestListController">
   <children>
      <Pane prefHeight="720.0" prefWidth="1280.0">
